<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <!-- 清除所有源 -->
    <clear />
    <!-- 添加本地包源 -->
    <add key="local" value="local-packages" />
    <!-- 保持 nuget.org 作为备用源 -->
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" />
  </packageSources>
  <!-- 禁用失效的私有源 -->
  <disabledPackageSources>
    <add key="laiye-private" value="https://pkgs.dev.azure.com/laiye/_packaging/your-feed/nuget/v3/index.json" />
  </disabledPackageSources>
</configuration>
